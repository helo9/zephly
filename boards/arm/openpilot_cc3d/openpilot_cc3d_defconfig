CONFIG_SOC_SERIES_STM32F1X=y
CONFIG_SOC_STM32F103XE=y

CONFIG_PINCTRL=y
CONFIG_PINCTRL_STM32=y

# enable uart driver
CONFIG_SERIAL=y
# enable console
CONFIG_CONSOLE=y
CONFIG_UART_CONSOLE=y

# enable pinmux
CONFIG_PINMUX=y

# disable jtag to get PB3 free
CONFIG_GPIO_STM32_SWJ_NOJTAG=y

# enable GPIO
CONFIG_GPIO=y

# enable clocks
CONFIG_CLOCK_CONTROL=y
